{
  "name": "replyoai-backend",
  "version": "1.0.0",
  "description": "Access control backend for ManyChat templates",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:worker": "tsx watch src/workers/index.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:worker": "node dist/workers/index.js",
    "start:all": "concurrently \"npm start\" \"npm run start:worker\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "lint": "tsc --noEmit",
    "lint:fix": "prettier --write src/",
    "metrics": "curl http://localhost:3000/metrics",
    "health": "curl http://localhost:3000/health",
    "precommit": "npm run lint && npm test"
  },
  "keywords": [
    "access-control",
    "manychat",
    "stripe"
  ],
  "author": "ReplyoAI",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@sentry/node": "^7.95.0",
    "@sentry/profiling-node": "^1.3.5",
    "bullmq": "^5.1.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "lru-cache": "^10.1.0",
    "nodemailer": "^6.9.7",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "prisma": "^5.22.0",
    "prom-client": "^15.1.0",
    "rate-limiter-flexible": "^4.0.1",
    "redis": "^4.6.12",
    "stripe": "^14.12.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.6",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
